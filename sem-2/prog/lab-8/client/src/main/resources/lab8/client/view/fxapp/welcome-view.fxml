<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lab8.client.view.fxapp.WelcomeController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Lab8">
                     <padding>
                        <Insets bottom="30.0" />
                     </padding>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#showLoginForm" prefHeight="27.0" prefWidth="150.0" text="%button.login">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#showRegisterForm" prefWidth="150.0" text="%button.register" />
               </children>
            </VBox>
         </center>
         <bottom>
            <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#showLanguagePicker" text="%universal.language" />
               </children>
            </HBox>
         </bottom>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </padding>
      </BorderPane>
   </children>
</AnchorPane>
